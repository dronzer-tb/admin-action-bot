# Discord Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_GUILD_ID=your_discord_server_id_here
DISCORD_BOT_CHANNEL_ID=your_bot_commands_channel_id_here
DISCORD_AUDIT_CHANNEL_ID=your_audit_log_channel_id_here

# Pterodactyl Configuration
PTERODACTYL_API_URL=https://your-panel.example.com
PTERODACTYL_API_KEY=your_pterodactyl_api_key_here
PTERODACTYL_SERVER_ID=your_minecraft_server_id_here

# Bot Configuration
ADMIN_ROLE_ID=your_admin_role_id_here
COMMAND_PREFIX=!

# Custom Minecraft Commands (Modify these to match your server's plugins)
# Default commands are for EssentialsX plugin
# Use {player}, {reason}, {duration} as placeholders where needed
CMD_KILL=kill {player}
CMD_KICK=kick {player} {reason}
CMD_TEMPBAN=tempban {player} {duration}m {reason}
CMD_IPBAN=ban {player} {reason}
CMD_MUTE=mute {player} {reason}
CMD_WARN=warn {player} {reason}
CMD_FREEZE=tick freeze
CMD_UNFREEZE=tick unfreeze
